   "source": [
    "import pyinputplus as pyip\n",
    "scores = pyip.inputNum(prompt=\"請輸入學生分數(0-100):\",blank = False,min=0, max=100)\n",
    "print(scores)\n",
    "\n",
    "if scores < 60:\n",
    "    print(f\"{scores}分:不及格\")\n",
    "if 80>scores>=60:\n",
    "     print(f\"{scores}分:及格\")\n",
    "if 100>=scores>=80:\n",
    "     print(f\"{scores}分:優\")"
   ]
  }
